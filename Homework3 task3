    <script>
        const Year = prompt("Введіть рік");
        let days = new Year().getFullYear();
        let daysInYear = days.getDays();

        if (year && daysInYear == 366) {
            alert("it is leap year")
        }
        else if (year && daysInYear < 366) {
            alert("It is not leap Year")
        } else {
            alert("Введіть інший рік")
        } 
    </script>

